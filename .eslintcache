 ModuleThreads from \"../components/ModuleThreadsListComp\";\n\nexport default function ViewFeedbackModuleList() {\n  return (\n    <div>\n      <AdminNavBar />\n      <ModuleThreads />\n    </div>\n  );\n}\n","C:\\Users\\FiercePC\\WebstormProjects\\epic-learning-and-engagement-tool\\src\\Admin\\pages\\AdminAnalytics.js",[],"C:\\Users\\FiercePC\\WebstormProjects\\epic-learning-and-engagement-tool\\src\\Admin\\pages\\AdminThreads.js",[],"C:\\Users\\FiercePC\\WebstormProjects\\epic-learning-and-engagement-tool\\src\\Admin\\pages\\AdminClasses.js",[],"C:\\Users\\FiercePC\\WebstormProjects\\epic-learning-and-engagement-tool\\src\\Admin\\pages\\AdminDiscussionBoard.js",[],"C:\\Users\\FiercePC\\WebstormProjects\\epic-learning-and-engagement-tool\\src\\Admin\\pages\\ViewFeedbackPage.js",[],"C:\\Users\\FiercePC\\WebstormProjects\\epic-learning-and-engagement-tool\\src\\firebase.js",[],"C:\\Users\\FiercePC\\WebstormProjects\\epic-learning-and-engagement-tool\\src\\Student\\NavBar\\StudentNavBar.js",["175"],"import React, { Component } from \"react\";\nimport \"./StudentNavBar.css\";\nimport MenuItems from \"./StudentMenuItems\";\nimport Logo from \"../../Images/WolfPack.png\";\n\nclass StudentNavBar extends Component {\n  state = { clicked: false };\n\n  render() {\n    return (\n      <nav className=\"NavbarItems\">\n        <div className=\"menu-icon\">\n          <img src={Logo} />\n        </div>\n        <ul>\n          {MenuItems.map((item, index) => {\n            return (\n              <li key={index}>\n                <a className={item.cName} href={item.url}>\n                  {item.title}\n                </a>\n              </li>\n            );\n          })}\n        </ul>\n      </nav>\n    );\n  }\n}\n\nexport default StudentNavBar;\n","C:\\Users\\FiercePC\\WebstormProjects\\epic-learning-and-engagement-tool\\src\\DataAnalysis\\Graph.js",["176"],"import React, { Component } from \"react\";\nimport {\n  LineChart,\n  Line,\n  CartesianGrid,\n  XAxis,\n  YAxis,\n  Tooltip,\n  Label,\n  ResponsiveContainer,\n} from \"recharts\";\nimport data from \"./Data\";\nimport CSS from \"./GraphElement.css\";\n\nclass Graph extends Component {\n  render() {\n    return (\n      <div className=\"Panel\">\n        <ResponsiveContainer className=\"MarkGraph\" width=\"90%\" height={600}>\n          <LineChart data={data}>\n            <Line\n              type=\"monotone\"\n              dataKey=\"IndividualMark\"\n              stroke=\"#BB86FC\"\n              strokeWidth=\"2px\"\n            />\n            <CartesianGrid stroke=\"#F8F8FF\" />\n            <XAxis\n              dataKey=\"Title\"\n              stroke=\"#F8F8FF\"\n              tick={{ dy: 7 }}\n              tickInterval=\"150\"\n              tickCount-=\"{data.length}\"\n            >\n              <Label\n                value=\"Assignments\"\n                position=\"insideBottom\"\n                offset={-15}\n                stroke=\"#F8F8FF\"\n              />\n            </XAxis>\n            <YAxis\n              domain={[0, 100]}\n              tickInterval=\"10\"\n              tickCount=\"11\"\n              stroke=\"#F8F8FF\"\n            >\n              <Label\n                value=\"Marks (%)\"\n                angle=\"-90\"\n                position=\"left\"\n                offset={-17.5}\n                stroke=\"#F8F8FF\"\n              />\n            </YAxis>\n            <Tooltip content={<CustomTooltip />} />\n          </LineChart>\n        </ResponsiveContainer>\n        <ul className=\"InfoSet\">\n          <li>\n            <h3> Progress </h3>\n          </li>\n        </ul>\n        <ul className=\"InfoSet\">\n          <h3> Detailed Summary </h3>\n        </ul>\n        <ul className=\"AssignmentList\">\n          {data.map((item, index) => {\n            return (\n              <li key={index} className=\"Assignment\">\n                <h5> {item.Title} </h5>\n                <p>\n                  Score: {item.IndividualMark} Me